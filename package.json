{
  "name": "bb26-spreadsheet",
  "version": "1.6.0",
  "description": "Bijective base-26 number system utilities for spreadsheet software",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/istareatscreens/bb26-spreadsheet.git"
  },
  "bugs": {
    "url": "https://github.com/istareatscreens/bb26-spreadsheet/issues"
  },
  "types": "lib",
  "files": [
    "lib"
  ],
  "keywords": [
    "bb26",
    "base-26",
    "spreadsheet",
    "excel",
    "bijective base-26",
    "Hexavigesimal"
  ],
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint src/*.ts",
    "test": "nyc --reporter=html --reporter=text env TS_NODE_COMPILER_OPTIONS='{\"noImplicitAny\": false }' mocha -r ts-node/register test/**/*.ts",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "sideEffects": false,
  "author": "Stephen Charbonneau",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "4.3.20",
    "@types/mocha": "9.1.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "chai": "4.5.0",
    "codecov": "3.8.3",
    "eslint": "8.57.1",
    "istanbul": "0.4.5",
    "mocha": "10.8.2",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "15.1.0",
    "prettier": "2.8.8",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.2",
    "typescript": "4.9.5"
  }
}
